class Main {
    public static void main(String[] args) {
        int arr[]={6,3,1,2,5};
        int evensum=0;
        int oddsum=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]%2==0){
                evensum+=arr[i];
            }else{
                oddsum+=arr[i];
            }
        }
        System.out.println(evensum-oddsum);
    }
}
