class Main {
    public static void main(String[] args) {
        int arr[]={6,3,1,2,4};
        int min=Integer.MAX_VALUE;
        int min2=Integer.MAX_VALUE;
        for(int i=0;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
            if(arr[i]<min2 && arr[i]!=min){
                    min2=arr[i];
                }
        }
        System.out.println(min2);
    }
}
